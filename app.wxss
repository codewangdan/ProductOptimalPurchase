@import "./styles/iconfont.wxss";

page,view,text,swiper,swiper-item,image,navigator{
  padding: 0;
  margin: 0;
  box-sizing: border-box;
}
/* 主题颜色 通过变量来实现
less 中存在变量这个知识 
原生css和wxss支持变量
--themeColor主题颜色
*/
page{
  --themeColor: #F8554D;
  /* 定义统一的字体大小 
  1px = 2rpx
  14px = 28rpx
  */
  font-size: 28rpx;
}
image{
  width: 100%;
}